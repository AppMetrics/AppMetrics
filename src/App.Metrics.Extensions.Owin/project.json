{
  "authors": ["Allan Hardy"],
  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "debugType": "full"
  },
  "dependencies": {
    "App.Metrics": "1.0.0-beta4",
    "Microsoft.Extensions.DependencyInjection": "1.1.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.PlatformAbstractions": "1.1.0",
    "NETStandard.Library": "1.6.1",
    "Owin": "1.0.0"    
  },
  "description":
    "Extends the App.Metrics library providing a set OWIN Middleware Components which will record things like error rates per endpoint and an overall rate, request rates per endpoint and an overall rate etc.",
  "frameworks": {
    "net452": {
      "buildOptions": {
        "define": [
          "CLASSIC"
        ]
      },
      "dependencies": {
      
      },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      }
    }
  },
  "packOptions": {
    "releaseNotes": "Initial Release",
    "tags": [
      "mvc",
      "measurements",
      "charts",
      "timer",
      "histogram",
      "graphite",
      "influxdb",
      "health checks",
      "metrics"
    ],
    "repository": {
      "type": "git",
      "url": "git://github.com/alhardy/AppMetrics"
    },
    "projectUrl": "https://alhardy.github.io/app-metrics-docs/getting-started/intro.html#configuring-a-web-host",
    "licenseUrl": "https://github.com/alhardy/AppMetrics/blob/master/LICENSE",
    "iconUrl": "https://alhardy.github.io/app-metrics-docs/logo.png"
  },
  "version": "1.0.0-beta4"

}
