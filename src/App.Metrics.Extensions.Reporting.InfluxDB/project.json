{
  "authors": ["Allan Hardy"],

  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "debugType": "full",
    "additionalArguments": [ "/ruleset:../../AppMetrics.ruleset", "/additionalfile:../../stylecop.json" ]
  },
  "copyright": "Allan Hardy 2016",

  "dependencies": {
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "App.Metrics": "1.0.0-beta5",
    "Microsoft.Extensions.DependencyInjection": "1.1.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.0",
    "Microsoft.Extensions.DependencyModel": "1.1.0",
    "Microsoft.Extensions.Options": "1.1.0",
    "Microsoft.Extensions.PlatformAbstractions": "1.1.0",
    "NETStandard.Library": "1.6.1",
    "Microsoft.DotNet.InternalAbstractions": "1.0.500-preview2-1-003177",
    "Polly": "4.3.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "description": "Provides InfluxDB reporting capability to App Metrics",

  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ],
      "dependencies": {
        "System.Collections.Specialized": "4.3.0",
        "System.Diagnostics.Process": "4.3.0",
        "System.Threading.Thread": "4.3.0"
      }
    },
    "net452": {
      "buildOptions": {
        "define": [
          "CLASSIC"
        ]
      },
      "dependencies": {
      
      },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      }

    }
  },

  "packOptions": {
    "tags": ["Metrics", "Monitoring", "Instrumentation", "Reporting", "InfluxDB"],
    "projectUrl": "https://alhardy.github.io/app-metrics-docs/getting-started/reporting/index.html#influxdb-reporter",
    "licenseUrl": "https://github.com/alhardy/AppMetrics/blob/master/LICENSE",
    "iconUrl": "https://alhardy.github.io/app-metrics-docs/logo.png",
    "repository": {
      "type": "git",
      "url": "git://github.com/alhardy/AppMetrics"
    }
  },
  "title": "App.Metrics.Extensions.Reporting.InfluxDB",
  "version": "1.0.0-beta5"
}
