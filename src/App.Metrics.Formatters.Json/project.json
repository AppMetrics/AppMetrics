{
  "version": "1.0.0-*",
  "description": "App.Metrics formatters for JSON output using Json.NET",
  "authors": ["Allan Hardy"],
  "packOptions": {
    "releaseNotes":
      "",
    "tags": [
      "app.metrics", "json"
    ],
    "repository": {
      "type": "git",
      "url": "git://github.com/alhardy/aspnet-metrics"
    },
    "projectUrl": "https://github.com/alhardy/aspnet-metrics",
    "licenseUrl": "https://github.com/alhardy/aspnet-metrics/blob/master/LICENSE",
    "iconUrl": "http://www.erata.net/Metrics.NET/metrics_32.png"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "debugType": "portable"
  },
  "dependencies": {
    "App.Metrics": "1.0.0-*",
    "Microsoft.DotNet.InternalAbstractions": "1.0.1-*",
    "NETStandard.Library": "1.6.0",
    "Newtonsoft.Json": "9.0.1",
    "System.Linq": "4.1.0"
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  }

}
